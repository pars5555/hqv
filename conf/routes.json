{
  
    "sale": [
     
        {
            "action": "loads.main.main",
            "route": "[:id]",
            "constraints": {
                "id": "[0-9]+"
            },
            "nestedLoad": {
                "content": {
                    "action": "loads.main.sale.open"
                }
            }
        }

    ],
  
    "default": {
        "action": "loads.main.index",
        "nestedLoad": {
            "content": {
                "action": "loads.main.home"
            }
        }
    }


}