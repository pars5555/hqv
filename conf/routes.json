{
    "lang": [
        {
            "action": "actions.main.set_language",
            "route": "[:lang]",
            "constraints": {
                "lang": "[a-zA-Z]+"
            }
        }
    ],
    "about": [
        {
            "action": "loads.main.index",
            "nestedLoad": {
                "content": {
                    "action": "loads.main.about"
                }
            }
        }

    ],
    "contact": [
        {
            "action": "loads.main.index",
            "nestedLoad": {
                "content": {
                    "action": "loads.main.contact"
                }
            }
        }

    ],
    "default": {
        "action": "loads.main.index",
        "nestedLoad": {
            "content": {
                "action": "loads.main.home"
            }
        }
    }


}